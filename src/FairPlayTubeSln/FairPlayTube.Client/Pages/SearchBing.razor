<h3>Search Bing</h3>

<Loading IsLoading="IsLoading"></Loading>

@if (this.AllResults != null)
{
    <div class="container">
        <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3">
            @foreach (var singleVideo in this.AllResults)
            {
                <div class="col">
                    @( (MarkupString)singleVideo.embedHtml)
                </div>
            }
        </div>
    </div>
}
